<Application xmlns="https://github.com/avaloniaui"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
 x:Class="App.App"
xmlns:local="clr-namespace:App"
 RequestedThemeVariant="Default">

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="Styles/DefaultStyle.axaml"></StyleInclude>
    </Application.Styles>
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Resources>
        <SolidColorBrush x:Key="PrimaryFG">#000000</SolidColorBrush>
        <SolidColorBrush x:Key="SecondaryFG">#fcfcfc</SolidColorBrush>
        <SolidColorBrush x:Key="PrimaryBG">#fcfcfc</SolidColorBrush>
        <SolidColorBrush x:Key="SecondaryBG">#cfcfcf</SolidColorBrush>
        <LinearGradientBrush x:Key="PrimaryBGGradient" StartPoint="0%,0%" EndPoint="100%,0%">
            <GradientStop Offset="0" Color="#1B3C53"></GradientStop>
            <GradientStop Offset="1" Color="#234C6A"></GradientStop>
        </LinearGradientBrush>
        <FontFamily x:Key="UbuntuSans">/Assets/Fonts/UbuntuSans.ttf#Ubuntu Sans Regular</FontFamily>
        <FontFamily x:Key="Phosphor">/Assets/Fonts/Phosphor.ttf#P</FontFamily>

     </Application.Resources>
</Application>
