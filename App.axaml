<Application xmlns="https://github.com/avaloniaui"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
 x:Class="App.App"
 RequestedThemeVariant="Default">
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="Styles/DefaultStyle.axaml"></StyleInclude>
    </Application.Styles>

    <Application.Resources>
        <SolidColorBrush x:Key="PrimaryFG">#cfcfcf</SolidColorBrush>
        <SolidColorBrush x:Key="PrimaryBG">#ffffff</SolidColorBrush>
        <LinearGradientBrush x:Key="PrimaryBGGradient" StartPoint="0%,0%" EndPoint="100%,0%">
            <GradientStop Offset="0" Color="#111214"></GradientStop>
            <GradientStop Offset="1" Color="#111214"></GradientStop>
        </LinearGradientBrush>
        <FontFamily x:Key="UbuntuSans">/Assets/Fonts/UbuntuSans#Ubuntu Sans Regular</FontFamily>

     </Application.Resources>
</Application>
